{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "sh vercel-install.sh",
  "buildCommand": "sh vercel-build.sh",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/((?!.*\\.[^/]+$).*)",
      "destination": "/index.html"
    }
  ]
}
